<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>



    <servlet>
        <servlet-name>pl.connectis.cschool.GWTApp InvoiceService</servlet-name>
        <servlet-class>pl.connectis.cschool.server.InvoiceServiceImpl</servlet-class>
    </servlet>




    <servlet-mapping>
        <servlet-name>pl.connectis.cschool.GWTApp InvoiceService</servlet-name>
        <url-pattern>/GWTApp/InvoiceService</url-pattern>
    </servlet-mapping>



    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>GWTApp.html</welcome-file>
    </welcome-file-list>


</web-app>
